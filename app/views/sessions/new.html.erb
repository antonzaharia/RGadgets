
<%= display_errors %>

<%= form_for(@user, url: sessions_path) do |f| %>

<%= f.label :email %>
<%= f.email_field :email %><br>

<%= f.label :password %>
<%= f.password_field :password %><br>

<%= f.submit "Login"%>
<% end %>

<p>Don't have an account yet? <%= link_to "Signup", new_user_path %></p>